{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"outDir": ".svelte-kit/output",
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	},
	"include": [
		".svelte-kit/ambient.d.ts",
		".svelte-kit/types/**/$types.d.ts",
		"./src/**/*.js",
		"./src/**/*.ts",
		"./src/**/*.svelte",
		"./tests/**/*.js",
		"./tests/**/*.ts",
		"./tests/**/*.svelte",
		"*.cjs",
		".*.cjs",
		"*.js",
		"*.ts",
		"**/*.tsx"
	]
}
