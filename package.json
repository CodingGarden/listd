{
	"name": "listd",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"dev": "concurrently \"vite dev\" typesafe-i18n",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "eslint --ext .js,.cjs,.ts,.svelte .",
		"format": "prettier --plugin-search-dir . --check .",
		"lint:fix": "eslint --ext .js,.cjs,.ts,.svelte . --fix",
		"format:fix": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.30.0",
		"@skeletonlabs/skeleton": "^0.92.2",
		"@sveltejs/adapter-auto": "^1.0.2",
		"@sveltejs/kit": "^1.3.10",
		"@tailwindcss/forms": "^0.5.3",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"autoprefixer": "^10.4.13",
		"concurrently": "^7.6.0",
		"eslint": "^8.33.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-import-resolver-typescript": "^3.5.3",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-svelte": "^2.15.0",
		"husky": "^8.0.3",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.2.2",
		"prisma": "^4.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-preprocess": "^5.0.1",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.5.0",
		"typesafe-i18n": "^5.21.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.1",
		"vitest": "^0.28.4"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "^0.4.0",
		"@auth/sveltekit": "^0.2.1",
		"@prisma/client": "^4.9.0",
		"@tabler/icons-svelte": "^2.2.0"
	},
	"license": "MIT",
	"engines": {
		"npm": ">= 7.0.0",
		"yarn": "Please use npm instead of yarn.",
		"pnpm": "Please use npm instead of pnpm."
	},
	"bugs": {
		"url": "https://github.com/CodingGarden/listd/issues"
	},
	"keywords": [
		"listd",
		"svelte",
		"typescript",
		"tailwindcss"
	],
	"author": "Coding Garden ('https://coding.garden/')",
	"repository": {
		"type": "git",
		"url": "git@github.com:CodingGarden/listd.git"
	}
}
