{
	"name": "listd",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"dev": "concurrently \"vite dev\" typesafe-i18n",
		"build": "vite build",
		"preview": "vite preview --host 0.0.0.0",
		"test": "npm run test:unit && npm run test:e2e",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest run",
		"test:e2e": "npm run docker-down:test && npm run docker-up:test",
		"test:e2e:watch": "npm run docker-down:test:watch && npm run docker-up:test:watch",
		"lint": "eslint --ext .js,.cjs,.ts,.svelte .",
		"typecheck": "npm run check",
		"format": "prettier --check .",
		"lint:fix": "eslint --ext .js,.cjs,.ts,.svelte . --fix",
		"format:fix": "prettier --write .",
		"migrate:init": "prisma migrate dev --name init",
		"docker-up:test": "docker compose -f docker-compose.test.yml up --build --abort-on-container-exit --exit-code-from app-test",
		"docker-down:test": "docker compose -f docker-compose.test.yml down",
		"docker-up:test:watch": "docker compose -f docker-compose.test.watch.yml up --build --abort-on-container-exit --exit-code-from app-test",
		"docker-down:test:watch": "docker compose -f docker-compose.test.watch.yml down",
		"docker-up:dev": "docker compose -f docker-compose.dev.yml up -d --wait",
		"docker-down:dev": "docker compose -f docker-compose.dev.yml down"
	},
	"prisma": {
		"seed": "tsx ./prisma/seeds/index.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.37.1",
		"@skeletonlabs/skeleton": "^2.0.0",
		"@skeletonlabs/tw-plugin": "^0.1.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.24.0",
		"@tailwindcss/forms": "^0.5.6",
		"@types/node": "^20.5.7",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"autoprefixer": "^10.4.15",
		"concurrently": "^8.2.1",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^7.3.0",
		"eslint": "^8.48.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-svelte": "^2.33.0",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"nyc": "^15.1.0",
		"postcss": "^8.4.29",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"prisma": "^5.2.0",
		"prisma-erd-generator-markdown": "^1.3.1",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"tsx": "^3.12.8",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^5.2.2",
		"typescript-svelte-plugin": "^0.3.31",
		"vite": "^4.4.9",
		"vite-plugin-istanbul": "^5.0.0",
		"vitest": "^0.34.3"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "^0.13.0",
		"@auth/sveltekit": "^0.3.6",
		"@googleapis/youtube": "^11.0.1",
		"@next-auth/prisma-adapter": "^1.0.7",
		"@prisma/client": "^5.2.0",
		"@types/luxon": "^3.3.2",
		"env-cmd": "^10.1.0",
		"luxon": "^3.4.2",
		"redis": "^4.6.8",
		"zod": "^3.22.2"
	},
	"license": "MIT",
	"engines": {
		"npm": ">= 7.0.0",
		"yarn": "Please use npm instead of yarn.",
		"pnpm": "Please use npm instead of pnpm."
	},
	"bugs": {
		"url": "https://github.com/CodingGarden/listd/issues"
	},
	"keywords": [
		"listd",
		"svelte",
		"typescript",
		"tailwindcss"
	],
	"author": "Coding Garden ('https://coding.garden/')",
	"repository": {
		"type": "git",
		"url": "git@github.com:CodingGarden/listd.git"
	}
}
