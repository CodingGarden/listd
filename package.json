{
	"name": "listd",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"dev": "concurrently \"vite dev\" typesafe-i18n",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:unit && npm run test:e2e",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest run",
    "test:e2e": "npm run docker-down:test && npm run docker-up:test && npm run migrate:init && cross-env NODE_ENV=test prisma db seed && npx playwright install && playwright test && npm run docker-down:test",
    "test:e2e:debug": "npm run docker-down:test && npm run docker-up:test && npm run migrate:init && cross-env NODE_ENV=test prisma db seed && npx playwright install && playwright test --debug && npm run docker-down:test",
		"lint": "eslint --ext .js,.cjs,.ts,.svelte .",
		"typecheck": "npm run check",
		"format": "prettier --plugin-search-dir . --check .",
		"lint:fix": "eslint --ext .js,.cjs,.ts,.svelte . --fix",
		"format:fix": "prettier --plugin-search-dir . --write .",
		"docker-up:test": "docker compose -f docker-compose.test.yml up -d --wait --force-recreate",
		"migrate:init": "prisma migrate dev --name init",
		"docker-down:test": "docker compose -f docker-compose.test.yml down"
	},
	"prisma": {
		"seed": "tsx ./prisma/seeds/index.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.33.0",
		"@skeletonlabs/skeleton": "^1.5.1",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/adapter-node": "^1.2.4",
		"@sveltejs/kit": "^1.16.3",
		"@tailwindcss/forms": "^0.5.3",
		"@typescript-eslint/eslint-plugin": "^5.59.5",
		"@typescript-eslint/parser": "^5.59.5",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.0.1",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^7.2.1",
		"eslint": "^8.40.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-svelte": "^2.28.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"prettier-plugin-tailwindcss": "^0.2.8",
		"prisma": "^4.14.0",
		"prisma-erd-generator-markdown": "^1.3.1",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.0",
		"tsx": "^3.12.7",
		"typesafe-i18n": "^5.24.3",
		"typescript": "^5.0.4",
		"typescript-svelte-plugin": "^0.3.25",
		"vite": "^4.3.5",
		"vitest": "^0.31.0"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "^0.7.1",
		"@auth/sveltekit": "^0.3.1",
		"@googleapis/youtube": "^9.0.0",
		"@next-auth/prisma-adapter": "^1.0.6",
		"@prisma/client": "^4.14.0",
		"@types/luxon": "^3.3.0",
		"env-cmd": "^10.1.0",
		"luxon": "^3.3.0",
		"redis": "^4.6.6",
		"zod": "^3.21.4"
	},
	"license": "MIT",
	"engines": {
		"npm": ">= 7.0.0",
		"yarn": "Please use npm instead of yarn.",
		"pnpm": "Please use npm instead of pnpm."
	},
	"bugs": {
		"url": "https://github.com/CodingGarden/listd/issues"
	},
	"keywords": [
		"listd",
		"svelte",
		"typescript",
		"tailwindcss"
	],
	"author": "Coding Garden ('https://coding.garden/')",
	"repository": {
		"type": "git",
		"url": "git@github.com:CodingGarden/listd.git"
	}
}
