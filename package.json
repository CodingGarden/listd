{
	"name": "listd",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"dev": "concurrently \"vite dev\" typesafe-i18n",
		"build": "vite build",
		"preview": "vite preview --host 0.0.0.0",
		"test": "npm run test:unit && npm run test:e2e",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest run",
		"test:e2e": "npm run docker-down:test && npm run docker-up:test",
		"test:e2e:watch": "npm run docker-down:test:watch && npm run docker-up:test:watch",
		"lint": "eslint --ext .js,.cjs,.ts,.svelte .",
		"typecheck": "npm run check",
		"format": "prettier --check .",
		"lint:fix": "eslint --ext .js,.cjs,.ts,.svelte . --fix",
		"format:fix": "prettier --write .",
		"migrate:init": "prisma migrate dev --name init",
		"migrate:deploy": "prisma migrate deploy",
		"docker-up:test": "docker compose -f docker-compose.test.yml up --force-recreate --build --abort-on-container-exit --exit-code-from app-test",
		"docker-down:test": "docker compose -f docker-compose.test.yml down",
		"docker-up:test:watch": "docker compose -f docker-compose.test.watch.yml up --build",
		"docker-down:test:watch": "docker compose -f docker-compose.test.watch.yml down",
		"docker-up:dev": "docker compose -f docker-compose.dev.yml up -d --wait",
		"docker-down:dev": "docker compose -f docker-compose.dev.yml down",
		"update:interactive": "npx npm-check-updates -i"
	},
	"prisma": {
		"seed": "tsx ./prisma/seeds/index.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.41.2",
		"@skeletonlabs/skeleton": "^2.8.0",
		"@skeletonlabs/tw-plugin": "^0.3.1",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/forms": "^0.5.7",
		"@types/node": "^20.11.16",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"autoprefixer": "^10.4.17",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^7.3.0",
		"eslint": "^8.56.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-svelte": "^2.35.1",
		"husky": "^9.0.10",
		"lint-staged": "^15.2.2",
		"nyc": "^15.1.0",
		"postcss": "^8.4.34",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"prisma": "^5.9.1",
		"prisma-erd-generator-markdown": "^1.3.1",
		"svelte": "^4.2.10",
		"svelte-check": "^3.6.3",
		"svelte-preprocess": "^5.1.3",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"tsx": "^4.7.0",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^5.3.3",
		"typescript-svelte-plugin": "^0.3.37",
		"vite": "^5.0.12",
		"vite-plugin-istanbul": "^5.0.0",
		"vitest": "^1.2.2"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "0.19.0",
		"@auth/sveltekit": "0.5.0",
		"@googleapis/youtube": "^13.1.0",
		"@next-auth/prisma-adapter": "^1.0.7",
		"@prisma/client": "^5.9.1",
		"@types/luxon": "^3.4.2",
		"@types/youtube-player": "^5.5.11",
		"anchorme": "^3.0.5",
		"env-cmd": "^10.1.0",
		"luxon": "^3.4.4",
		"redis": "^4.6.13",
		"youtube-player": "^5.6.0",
		"zod": "^3.22.4"
	},
	"license": "MIT",
	"engines": {
		"npm": ">= 7.0.0",
		"yarn": "Please use npm instead of yarn.",
		"pnpm": "Please use npm instead of pnpm."
	},
	"bugs": {
		"url": "https://github.com/CodingGarden/listd/issues"
	},
	"keywords": [
		"listd",
		"svelte",
		"typescript",
		"tailwindcss"
	],
	"author": "Coding Garden ('https://coding.garden/')",
	"repository": {
		"type": "git",
		"url": "git@github.com:CodingGarden/listd.git"
	}
}
